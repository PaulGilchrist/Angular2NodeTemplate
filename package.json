{
  "authors": [
    "Paul Gilchrist"
  ],
  "name": "angular2-template",
  "description": "Angular2 Template using npm for packages, gulp as the task runner",
  "homepage": "https://apiclient2.azurewebsites.net",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PaulGilchrist/Angular2VSTemplate.git"
  },
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "lint": "tslint ./src/app/**/*.ts -t verbose",
    "postinstall": "npm run typings install",
    "test": "karma start",
    "typings": "typings",
    "start": "gulp copyFiles && webpack-dev-server --inline --progress --content-base build/ --port 3000",
    "build": "gulp copyFiles && webpack --config config/webpack.prod.js --progress --profile --bail && cd build && npm i && cd ../"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "animate.css": "^3.5.2",
    "core-js": "^2.4.1",
    "bootstrap": "^3.3.7",
    "d3": "^3.5.17",
    "dragula": "^3.7.2",
    "font-awesome": "^4.6.3",
    "jquery": "^3.1.0",
    "jquery-confirm": "^1.5.1",
    "ng2-ckeditor": "^1.0.7",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "toastr": "^2.1.2",
    "underscore": "^1.8.3",
    "zone.js": "0.6.23"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.5.0",
    "canonical-path": "0.0.2",
    "css-loader": "^0.25.0",
    "del": "^2.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "html-loader": "^0.4.4",
    "http-server": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "jasmine-core": "^2.5.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "lodash": "^4.15.0",
    "npm-flatten": "^0.0.1",
    "null-loader": "^0.1.1",
    "protractor": "^4.0.8",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "typescript": "^1.8.10",
    "typings": "^1.3.3",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.2",
    "webpack-merge": "^0.14.1"
  }
}
