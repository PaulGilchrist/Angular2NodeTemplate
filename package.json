{
  "authors": [
    "Paul Gilchrist"
  ],
  "name": "angular2-template",
  "description": "Angular2 Template using npm for packages, gulp as the task runner",
  "homepage": "https://apiclient2.azurewebsites.net",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PaulGilchrist/Angular2VSTemplate.git"
  },
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "lint": "tslint ./src/app/**/*.ts -t verbose",
    "lite": "lite-server",
    "postinstall": "npm run typings install",
    "start": "gulp build && concurrently \"gulp watch\" \"npm run lite\" \"npm run test\" ",
    "test": "cd build && live-server --open=unit-tests.html --port=4000",
    "test2": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "0.2.0",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "animate.css": "^3.5.1",
    "core-js": "^2.4.0",
    "bootstrap": "^3.3.6",
    "d3": "^3.5.17",
    "dragula": "^3.7.1",
    "font-awesome": "^4.6.3",
    "jquery": "^2.2.4",
    "jquery-confirm": "^1.5.1",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "toastr": "^2.1.2",
    "underscore": "^1.8.3",
    "zone.js": "0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "canonical-path": "0.0.2",
    "del": "^2.2.1",
    "gulp": "^3.9.1",
    "gulp-htmlmin": "^2.0.0",
    "gulp-clean-css": "^2.0.10",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^2.13.6",
    "gulp-uglify": "^1.5.4",
    "gulp-util": "^3.0.7",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-cli": "^0.1.2",
    "karma-htmlfile-reporter": "^0.2.2",
    "karma-jasmine": "^0.3.8",
    "lite-server": "^2.2.2",
    "lodash": "^4.11.1",
    "merge2": "^1.0.2",
    "npm-flatten": "^0.0.1",
    "protractor": "^3.3.0",
    "rimraf": "^2.5.2",
    "run-sequence": "^1.2.1",
    "tslint": "^3.7.4",
    "typescript": "^1.8.10",
    "typings": "^1.3.0"
  }
}
