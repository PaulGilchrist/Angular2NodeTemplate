  "version": "1.0.0",
  "scripts": {
    "lint": "tslint ./src/app/**/*.ts -t verbose",
    "test": "karma start",
    "start": "gulp copyFiles && webpack-dev-server --inline --progress --content-base build/ --port 3000",
    "build": "gulp copyFiles && webpack --config config/webpack.prod.js --progress --profile --bail && cd build && npm i && cd ../"
