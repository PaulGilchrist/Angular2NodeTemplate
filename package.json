{
  "authors": [
    "Paul Gilchrist"
  ],
  "name": "angular2-template",
  "description": "Angular2 Template using npm for packages, gulp as the task runner",
  "homepage": "https://apiclient2.azurewebsites.net",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PaulGilchrist/Angular2VSTemplate.git"
  },
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "lint": "tslint ./src/app/**/*.ts -t verbose",
    "lite": "lite-server",
    "postinstall": "npm run typings install",
    "show-test-results": "live-server --open=build\\_test-output\\tests.html --port=4000",
    "start": "gulp build && concurrently \"gulp watch\" \"lite-server\"",
    "test": "npm run test-once && npm run show-test-results",
    "test-once": "karma start karma.conf.js --single-run",
    "typings": "typings"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.6",
    "@angular/compiler": "2.0.0-rc.6",
    "@angular/core": "2.0.0-rc.6",
    "@angular/forms": "2.0.0-rc.6",
    "@angular/http": "2.0.0-rc.6",
    "@angular/platform-browser": "2.0.0-rc.6",
    "@angular/platform-browser-dynamic": "2.0.0-rc.6",
    "@angular/router": "3.0.0-rc.2",
    "animate.css": "^3.5.1",
    "core-js": "^2.4.1",
    "bootstrap": "^3.3.7",
    "d3": "^3.5.17",
    "dragula": "^3.7.1",
    "font-awesome": "^4.6.3",
    "jquery": "^3.1.0",
    "jquery-confirm": "^1.5.1",
    "reflect-metadata": "^0.1.4",
    "rxjs": "5.0.0-beta.11",
    "systemjs": "0.19.27",
    "toastr": "^2.1.2",
    "underscore": "^1.8.3",
    "zone.js": "0.6.17"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "canonical-path": "0.0.2",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-htmlmin": "^2.0.0",
    "gulp-clean-css": "^2.0.12",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^2.13.6",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "lite-server": "^2.2.2",
    "lodash": "^4.15.0",
    "merge2": "^1.0.2",
    "npm-flatten": "^0.0.1",
    "protractor": "^3.3.0",
    "rimraf": "^2.5.2",
    "run-sequence": "^1.2.2",
    "tslint": "^3.14.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.2"
  }
}
